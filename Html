<?php
// Функция для вывода таблицы умножения
function printMultiplicationTable($size = 10) {
    echo "<table border='1'>";
    for ($i = 1; $i <= $size; $i++) {
        echo "<tr>";
        for ($j = 1; $j <= $size; $j++) {
            echo "<td>" . $i * $j . "</td>";
        }
        echo "</tr>";
    }
    echo "</table>";
}

// Получение размера таблицы из GET-запроса
$size = isset($_GET['size']) ? intval($_GET['size']) : 10;

// Вывод таблицы
printMultiplicationTable($size);
?>
