%include "io.inc"

section .text
global CMAIN
CMAIN:
    PRINT_STRING "a: "
    GET_DEC 4, eax          ; a → eax

    PRINT_STRING "b: "
    GET_DEC 4, ebx          ; b → ebx

    cdq                     ; расширяем знак eax в edx:edx:eax
    idiv ebx                ; eax ← частное, edx ← остаток

    PRINT_STRING "*: "
    PRINT_DEC 4, eax
    NEWLINE

    PRINT_STRING "%: "
    PRINT_DEC 4, edx
    NEWLINE

    xor eax, eax
    ret
