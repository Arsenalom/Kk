%include "io.inc"

section .text
global CMAIN
CMAIN:
    PRINT_STRING "number: "
    GET_DEC 4, eax

    test al, 1              ; проверяем младший бит
    jz .even

    ; нечётное
    PRINT_STRING "result: 0 (Nechet)"
    jmp .end

.even:
    PRINT_STRING "result 1 (chet)"

.end:
    NEWLINE
    xor eax, eax
    ret
