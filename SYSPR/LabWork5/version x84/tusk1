%include "io.inc"

section .data
    prompt db "Введите количество строк: ", 0

section .bss
    rows resd 1

section .text
    global CMAIN

CMAIN:
    PRINT_STRING prompt
    NEWLINE
    GET_UDEC 4, [rows]

    mov ebx, 1              ; текущая строка

outer:
    cmp ebx, [rows]
    jg done

    mov ecx, ebx            ; сколько # напечатать

inner:
    PRINT_CHAR '#'
    loop inner

    NEWLINE
    inc ebx
    jmp outer

done:
    xor eax, eax            ; return 0
    ret
